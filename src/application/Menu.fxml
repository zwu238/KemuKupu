      <Button layoutX="187.0" layoutY="250.0" mnemonicParsing="false" onAction="#startGame" prefHeight="50.0" prefWidth="300.0" styleClass="buttonMain" stylesheets="@application.css" text="Play Game" textFill="#bc0000">
